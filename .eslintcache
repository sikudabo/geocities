t list item that shows the community moderator */}\n                                    <ListItem\n                                        style={{\n                                            cursor: 'pointer',\n                                        }}\n                                        onClick={e => history.push(`/geouser/${community.moderator.uniqueUserId}`)}\n                                        alignItems='flex-start'\n                                    >\n                                        <ListItemAvatar>\n                                            <Avatar \n                                                src={`https://www.geocities.cc/api/get/avatar/by/id/${community.moderator.uniqueUserId}`}\n                                                title={`${community.moderator.username}`}\n                                                alt={`${community.moderator.username}`} \n                                                style={{\n                                                    cursor: 'pointer',\n                                                }}\n                                            />\n                                        </ListItemAvatar>\n                                        <ListItemText \n                                            primary={\n                                                <div>\n                                                    <Typography \n                                                        variant='h6' \n                                                        component='h6' \n                                                    >\n                                                        Moderator \n                                                    </Typography>\n                                                    <Typography \n                                                        variant='subtitle1' \n                                                        component='span' \n                